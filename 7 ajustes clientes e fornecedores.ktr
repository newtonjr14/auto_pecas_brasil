            C.TELEFONE,
            C.FAX,
            'S',
            C.INSC_ESTADUAL,
            SUBSTR(C.ENDERECO,1,40) AS NOME,
            C.COD_CIDADE AS CODIGO_CIDADE,
            CV.UF,
            C.CODIGO_LOGRADOURO
        FROM CLIENTE C
